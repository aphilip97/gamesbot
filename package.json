  "description": "A bot that you can ping to let everyone on the server know that you're still alive.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "nodemon -r dotenv/config ./dist/index.js",
