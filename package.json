  "bugs": {
    "url": "https://github.com/aphilip97/gamesbot/issues"
  },
  "homepage": "https://github.com/aphilip97/gamesbot#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/ws": "^7.4.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0"
