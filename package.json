    "test": "jest --detectOpenHandles --runInBand --coverage",
    "test:watch": "jest --detectOpenHandles --runInBand --coverage --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aphilip97/gamesbot.git"
  },
